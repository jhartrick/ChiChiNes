{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "../dist/workers/",
    //"outFile": "../src/assets/emulator.worker.js",
    "sourceMap": false,
    "declaration": false,
    "moduleResolution": "node",
    "module": "commonjs",
    "emitDecoratorMetadata": true,
